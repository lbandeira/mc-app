{
  "expo": {
    "name": "mc-app",
    "slug": "mc-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_connect.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_connect.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.itemmrecife.mcapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_connect.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7518561e-0fb2-4884-a2a1-890994dfe571"
      }
    }
  }
}
